{
	"name": "christiano-property-management",
	"private": true,
	"version": "2.0.0",
	"type": "module",
	"description": "Enterprise-grade Property Management CMS for Christiano PM",
	"author": "Christiano Property Management",
	"scripts": {
		"dev": "bunx vite",
		"build": "bunx vite build",
		"build:dev": "bunx vite build --mode development",
		"build:analyze": "bunx vite build --mode analyze",
		"preview": "bunx vite preview",
		"test": "bunx vitest run",
		"test:watch": "bunx vitest",
		"test:e2e": "bunx playwright test",
		"test:e2e:ui": "bunx playwright test --ui",
		"test:e2e:debug": "bunx playwright test --debug",
		"type-check": "bunx tsc --noEmit",
		"lint": "bunx biome check . --apply",
		"lint:check": "bunx biome check .",
		"format": "bunx biome format . --write",
		"format:check": "bunx biome format .",
		"clean": "bunx rimraf dist node_modules/.vite",
		"start": "bun run dev",
		"prod": "bun run build && bun run preview",
		"security:audit": "bun audit",
		"deps:update": "bunx npm-check-updates -u",
		"perf:bundle": "bun run build:analyze",
		"perf:lighthouse": "bunx lighthouse http://localhost:8080 --output html --output-path ./reports/lighthouse.html",
		"docker:build": "docker build -t Christiano-cms .",
		"docker:run": "docker run -p 8080:8080 Christiano-cms"
	},
	"workspaces": ["src/**"],
	"packageManager": "bun@1.1.34",
